                echo 'Hello World from $Name $Surname'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Realease') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
